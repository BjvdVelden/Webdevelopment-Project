@model IEnumerable<Intake>

@{
    ViewData["Title"] = "Mijn Aanmeldingen";
}

<h1>Mijn Aanmeldingen</h1>

@* <p>
    <a asp-action="Create">Create New</a>
</p> *@
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Voornaam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Achternaam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GeboorteDatum)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BSN)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GewensteHulpverlener)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GewensteMoment)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Onderwerp)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Voornaam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Achternaam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GeboorteDatum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BSN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GewensteHulpverlener)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GewensteMoment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Onderwerp)
            </td>
            <td>
                @* <a asp-action="Edit" asp-route-id="@item.AanmeldingId">Edit</a> | *@
                <a asp-action="Details" asp-route-id="@item.IntakeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IntakeId">Verwijderen</a>
            </td>
        </tr>
}
    </tbody>
</table>