@model Intake

@{
    ViewData["Title"] = "Verwijderen intake";
}

<h1>Verwijderen intake</h1>

<h2>Weet je zeker dat je intake wilt verwijderen?</h2>
<div>
    <h3>Aanmelding</h3>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Voornaam)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Voornaam)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Achternaam)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Achternaam)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.GeboorteDatum)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.GeboorteDatum)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.BSN)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BSN)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.GewensteHulpverlener)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.GewensteHulpverlener)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.GewensteMoment)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.GewensteMoment)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Onderwerp)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Onderwerp)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IntakeId" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="IndexIntake">Terug</a>
    </form>
</div>