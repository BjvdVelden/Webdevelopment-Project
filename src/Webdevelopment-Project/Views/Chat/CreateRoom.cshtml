@model IEnumerable<Chat>

    <div>
        <a asp-controller="Chat" asp-action="FindGroup">Find Groups</a>
        <a asp-controller="Chat" asp-action="FindUser">Find Users</a>
        <a asp-controller="Chat" asp-action="Private">Private</a>
        <a asp-controller="Chat" asp-action="CreateRoom">Create</a>
    </div>


    <div>
        <form asp-controller="Chat" asp-action="CreateRoom">
            <header>Create Room</header>
            <div>
                Naam <input type="text" name="name">
                MinimumLeeftijd <input type="number" name="minimumage">
                MaximumLeeftijd <input type="number" name="maximumage">
            <footer>
                <button>Create</button>
            </footer>
        </form>
    </div>