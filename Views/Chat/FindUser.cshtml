@model IEnumerable<ApplicationUser>

    <div>
        <a asp-controller="Chat" asp-action="Index">Chat</a>
        <a asp-controller="Chat" asp-action="FindUser">Find Users</a>
        <a asp-controller="Chat" asp-action="Private">Private</a>
    </div>

    @foreach(var user in Model){
    <div>
        <a asp-controller="Chat" asp-action="CreatePrivateRoom" asp-route-userId="@user.Id">
            @user.UserName
            <a class="nav-link" asp-area="" asp-controller="Report" asp-action="Create"> Meld misbruik </a>
        </a>
    </div>
    
    }